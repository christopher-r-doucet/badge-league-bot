 {
  "name": "badge-league-bot",
  "version": "1.0.0",
  "description": "Discord bot for managing badge leagues",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development node --import ./register.js src/index.ts",
    "watch": "tsc -w",
    "postinstall": "npm run build",
    "deploy-commands": "NODE_ENV=development node --import ./register.js src/deploy-commands.ts"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "ts-node": "^10.9.2"
  }
}
